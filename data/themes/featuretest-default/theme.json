{
	"values": {
		"logo_scale": 0.3,
		"logo_width": {"multiply": {"value0" : "image.width_origin", "value1": "logo_scale"}},
		"logo_height": {"multiply": {"value0" : "image.height_origin", "value1": "logo_scale"}},
		"logo_x": {"add": {
			"value0": "screen.left",
			"value1": {"multiply": {"value0" : "logo_width", "value1": 0.75}}
		}},
		"logo_y": {"add": {
			"value0": "screen.top",
			"value1": {"multiply": {"value0" : "logo_height", "value1": 0.75}}
		}},
		"logo_x_start": {"subtract": {"value0": "screen.left", "value1": "logo_width"}},
		"logo_x_end": "logo_x",
		"scale_logo_on_enter": {
			"add": {
				"value0": "logo_scale",
				"value1": {
					"multiply": {
						"value0": {
							"sinus": { "degree": { "multiply": {
								"value0": {
									"min": [ {"time": ""}, 1.0 ]
								},
								"value1": 180
						}}}},
						"value1": 0.1
					}
				}
			}
		},
		"popup_logo_on_enter": {
			"sinus": {
				"degree": {
					"multiply": {
						"value0": {
							"min": [ {"time": ""}, 1.0 ]
						},
						"value1": 180.0
					}
				}
			}
		}
	},
